<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qinfei.qferp.mapper.dsg.DsgUserMapper">
    <select id="ds" resultType="com.qinfei.qferp.entity.sys.User">
        SELECT * FROM sys_user
    </select>

    <insert id="dsg" parameterType="com.qinfei.qferp.entity.sys.User">
        INSERT INTO sys_user(user_name,password,name) VALUES (#{userName},#{password},#{name})
    </insert>
    <update id="up" parameterType="com.qinfei.qferp.entity.sys.User">
        update sys_user set name=#{name} where id=1002;
    </update>
</mapper>