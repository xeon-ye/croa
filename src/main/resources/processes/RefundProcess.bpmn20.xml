<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:flowable="http://flowable.org/bpmn"
             xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
             xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC"
             xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI"
             targetNamespace="http://www.flowable.org/processdef">
    <process id="refund" name="退款申请" isExecutable="true">
        <documentation>退款申请的流程。</documentation>
        <startEvent id="startEvent" name="启动流程"/>
        <sequenceFlow id="sid-D4B5EDBF-D8C0-484B-8FF3-3EB289E9C28E" name="开始" sourceRef="startEvent"
                      targetRef="leaderApprove">
        </sequenceFlow>

        <userTask id="leaderApprove" name="直属领导审核">
            <extensionElements>
                <flowable:taskListener event="create" class="com.qinfei.qferp.flow.listener.LeaderTaskHandler"/>
            </extensionElements>
        </userTask>
        <sequenceFlow id="sid-16EB2C16-2682-46DD-B318-AAEA76C224DA" name="通过" sourceRef="leaderApprove"
                      targetRef="bursarApprove">
        </sequenceFlow>

        <userTask id="bursarApprove" name="会计审核">
            <extensionElements>
                <flowable:taskListener event="create" class="com.qinfei.qferp.flow.listener.BursarTaskHandler"/>
            </extensionElements>
        </userTask>
        <sequenceFlow id="sid-3DAABF14-389B-4B35-B05D-21EB0BD03CC3" name="通过" sourceRef="bursarApprove"
                      targetRef="financeApprove">
        </sequenceFlow>

        <userTask id="financeApprove" name="财务总监审核">
            <extensionElements>
                <flowable:taskListener event="create" class="com.qinfei.qferp.flow.listener.FinanceTaskHandler"/>
                <flowable:taskListener event="complete" class="com.qinfei.qferp.flow.listener.RefundMoneyGateHandler"/>
            </extensionElements>
        </userTask>
        <sequenceFlow id="sid-C5749AC3-1918-4FC7-84D0-AA58F10642A5" name="通过" sourceRef="financeApprove"
                      targetRef="judgeMoney">
        </sequenceFlow>

        <exclusiveGateway id="judgeMoney" name="是否大于1000000"/>
        <sequenceFlow id="sid-18959305-5EE9-417C-94D2-F034ED8BFB28" name="是" sourceRef="judgeMoney"
                      targetRef="managerApprove">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${gateCheck}]]></conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="sid-1E1ECEA6-D267-4545-8C1F-22382CFE6DEF" name="否" sourceRef="judgeMoney"
                      targetRef="endEvent">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!gateCheck}]]></conditionExpression>
        </sequenceFlow>

        <userTask id="managerApprove" name="总经理审核">
            <extensionElements>
                <flowable:taskListener event="create" class="com.qinfei.qferp.flow.listener.ManagerTaskHandler"/>
                <flowable:taskListener event="complete" class="com.qinfei.qferp.flow.listener.ProcessEndHandler"/>
            </extensionElements>
        </userTask>

        <sequenceFlow id="sid-6AF7CE47-E5FC-4E09-8F13-AD9367EAC933" name="结束" sourceRef="managerApprove"
                      targetRef="endEvent">
        </sequenceFlow>

        <endEvent id="endEvent" name="结束流程"/>
    </process>
    <bpmndi:BPMNDiagram id="BPMNDiagram_refund">
        <bpmndi:BPMNPlane bpmnElement="refund" id="BPMNPlane_refund">
            <bpmndi:BPMNShape bpmnElement="startEvent" id="BPMNShape_startEvent">
